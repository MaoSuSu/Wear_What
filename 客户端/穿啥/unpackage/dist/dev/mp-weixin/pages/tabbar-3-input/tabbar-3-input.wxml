<view><cu-custom vue-id="1" bgColor="bg-gradual-red" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">录入</view></cu-custom><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<1}}"><view data-event-opts="{{[['tap',[['AddImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="cu-form-group"><text class="cuIcon-tag text-red"></text><input type="text" placeholder="   填写衣服名称" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="cu-form-group margin-top"><text class="cuIcon-list text-red"></text><view><picker mode="selector" range="{{type}}" data-event-opts="{{[['change',[['typeChange',['$event']]]]]}}" bindchange="__e"><view>{{type[currentTypeIndex]}}</view></picker></view></view><view class="cu-form-group"><text class="cuIcon-skin text-red"></text><view><picker mode="selector" range="{{color}}" data-event-opts="{{[['change',[['colorChange',['$event']]]]]}}" bindchange="__e"><view>{{color[currentColorIndex]}}</view></picker></view></view><view class="cu-form-group"><text class="cuIcon-global text-red"></text><view><picker mode="selector" range="{{season}}" data-event-opts="{{[['change',[['seasonChange',['$event']]]]]}}" bindchange="__e"><view>{{season[currentSeasonIndex]}}</view></picker></view></view><view class="submitNow padding flex flex-direction margin-top-xl"><button data-event-opts="{{[['tap',[['submitNow',['$event']]]]]}}" class="cu-btn round bg-red shadow-blur lg margin-top-xl" bindtap="__e">提交</button></view></view>